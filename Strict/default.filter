ItemDisplay[GEM=1]:																	// Hide Chipped Gems									
ItemDisplay[GEM=2]:																	// Hide Flawed Gems
ItemDisplay[GEM=3]:																	// Hide Normal Gems

// Flawless (Order: Amethyst, Diamond, Emerald, Ruby, Sapphire, Skull, Topaz)
