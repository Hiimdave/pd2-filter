//-----------------------------RUNES-----------------------------
// General
ItemDisplay[RUNE<11]: %GREEN%%RUNENAME% %GREEN%(%RUNENUM%)%CONTINUE%
ItemDisplay[RUNE>10 RUNE<24]: %ORANGE%%RUNENAME% %ORANGE%(%RUNENUM%)%CONTINUE%
ItemDisplay[RUNE>23]: %RED%%RUNENAME% %RED%(%RUNENUM%)%CONTINUE%

// High Value Runes !!!IMPORTANT - Leave the space around %NAME% or game crashes on mouseover, no clue why!!!
ItemDisplay[RUNE>20 AND RUNE<26]: %GREEN%+%YELLOW%+%RED%+ %NAME% %RED%+%YELLOW%+%GREEN%+%WHITE%%CONTINUE%
ItemDisplay[RUNE>25]:  %YELLOW%++%RED%++%PURPLE%++ %NAME% %PURPLE%++%RED%++%YELLOW%++%WHITE%%CONTINUE%

// Add pick me up tag
ItemDisplay[RUNE>20]: %NAME%%NL%%NL%%PURPLE%***  [%TAN%pick me up%PURPLE%]  ***%WHITE%%CONTINUE%

// Add Notification
ItemDisplay[RUNE>20]: %NAME%%WHITE%%DOT-76%%CONTINUE%

